<Window x:Class="WpfApp3.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp3"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <StackPanel  Grid.Row="0">
            <Button x:Name="Loadinfo" Margin="20" Padding="5" Content="Load info" Click="Loadinfo_Click"/>
            <TextBlock x:Name="textblock1" Padding="5" Text="smilik" Margin="20"/>
        </StackPanel>
        
      
        <DataGrid  Grid.Row="1" x:Name="datagrid1" Margin="20" Padding="5" AutoGenerateColumns="True">
        </DataGrid>
     
    </Grid>
</Window>
